{
  "name": "autochecks-js-codecept-todomvc-react",
  "version": "1.0.0",
  "description": "ToDoMVC React Automated Checks using Javascript and CodeceptJS",
  "main": "index.js",
  "scripts": {
    "server": "npx selenium-standalone start",
    "headless": "codeceptjs run --config=./codecept.headless.js --reporter mocha-multi",
    "headless-parallel": "codeceptjs run-workers 4 --config=./codecept.headless.js",
    "local": "codeceptjs run --config=./codecept.local.js --reporter mocha-multi",
    "local-parallel": "codeceptjs run-workers 4 --config=./codecept.local.js",
    "test:headless": "start-server-and-test server 4444 headless",
    "test:headless-parallel": "start-server-and-test server 4444 headless-parallel",
    "test:local": "start-server-and-test server 4444 local",
    "test:local-parallel": "start-server-and-test server 4444 local-parallel",
    "test:browserstack": "codeceptjs run --config=./codecept.browserstack.js --reporter mocha-multi",
    "test:browserstack-parallel": "codeceptjs run-workers 4 --config=./codecept.browserstack.js"
  },
  "author": "Jason Ogayon",
  "license": "ISC",
  "dependencies": {
    "@codeceptjs/configure": "^0.5.2",
    "@codeceptjs/ui": "^0.4.2",
    "axios": "^0.21.1",
    "browserstack-local": "^1.4.8",
    "chai": "^4.2.0",
    "codeceptjs": "^2.6.10",
    "codeceptjs-testrail": "^1.7.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "mocha": "^8.1.1",
    "mocha-multi": "^1.1.3",
    "mochawesome": "^6.1.1",
    "moment": "^2.26.0",
    "node-cron": "^2.0.3",
    "selenium-standalone": "^8.0.4",
    "start-server-and-test": "^1.14.0",
    "unirest": "^0.6.0",
    "webdriverio": "^6.5.2"
  }
}
